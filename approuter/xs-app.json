{
  "welcomeFile": "/mtbeershopstatic/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/logout",
    "logoutPage": "/mtbeershopstatic/logout.html"
  },
  "routes": [
    {
      "source": "/catalog/(.*)$",
      "target": "/catalog/$1",
      "authenticationType": "xsuaa",
      "destination": "srv",
      "csrfProtection": true
    },
    {
      "source": "^/destination/([^/]+)/(.*)$",
      "target": "$2",
      "destination": "$1"
    },
    {
      "source": "/sap/(.*)$",
      "target": "/sap/$1",
      "destination": "S4HANA"
    },
    {
      "source": "^/mtbeershopstatic/logout.html$",
      "target": "/mtbeershopstatic/logout.html",
      "service": "html5-apps-repo-rt",
      "authenticationType": "none"
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa"
    }
  ]
}
