{
  "welcomeFile": "/mtbeershopstatic/",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout",
    "logoutPage": "/mtbeershopstatic/logout.html"
  },
  "whitelistService": {
    "endpoint": "/allowlist/service"
  },
  "routes": [
    {
      "source": "^/user-api(.*)",
      "target": "$1",
      "service": "sap-approuter-userapi"
    },
    {
      "source": "/basic-auth/(.*)$",
      "target": "/$1",
      "destination": "srv",
      "authenticationType": "none"
    },
    {
      "source": "/catalog/(.*)$",
      "target": "/catalog/$1",
      "destination": "srv",
      "csrfProtection": true
    }
  ]
}
